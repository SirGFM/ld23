	public class PlayState extends FlxState {
		
		[Embed(source = "../../assets/gfx/smallTile.png")] private var tile01:Class;
		[Embed(source = "../../assets/apple.mp3")] private var sfx:Class;
		
		static public var curState:uint = 0;
		static public var win:Boolean = true;
		
		private var player:Player;
		private var yummy:Yummy;
		private var world:FlxTilemap;
		private var mob:FlxGroup;
		private var text:FlxText;
		
		private var next:NextStageMenu;
		private var objects:FlxGroup;
		
		public function PlayState(n:uint = 0) {
			objects = new FlxGroup();
			mob = new FlxGroup();
			world = new FlxTilemap();
			text = new FlxText(20, 40, 210, "");
			yummy = new Yummy();
			next = new NextStageMenu();
			next.kill();
			curState = n;
			FlxG.addPlugin(new ResetPlugin());
		}
		
		override public function destroy():void {
			super.destroy();
			objects = null;
			FlxG.removePluginType(ResetPlugin);
		}
		
		override public function create():void {
			FlxG.bgColor = 0xff1033bb;
			player = new Hummy(50, 50, new FlxPoint(75, 100));
			objects.add(player);
			objects.add(mob);
			world.loadMap(FlxTilemap.arrayToCSV(uglyStageCreate(curState), 32), tile01, 8, 8);
			add(world);
			add(mob);
			add(player);
			add(text);
			mob.add(yummy);
			FlxG.watch(player, "size");
		}
		
		override public function draw():void {
			super.draw();
			if (next.alive) {
				next.draw();
			}
		}
		
		override public function update():void {
			if (next.alive) {
				//next.update();
				setState(curState);
				next.kill();
				return;
			}
			FlxG.collide(objects, world, worldCol);
			FlxG.overlap(player, mob, mobCol);
			super.update();
			if (!player.alive) {
				win = false;
				next.revive();
			}
			else if (!yummy.alive) {
				curState++;
				win = true;
				next.revive();
			}
		}
		
		private function worldCol(obj:BasicObj, w:FlxTilemap):void {
			if (obj is Player) {
				var size:Number = w.width / w.widthInTiles; 
				if (player.eat && (player.size >= size)) {
					if (((player.facing == FlxObject.LEFT) && (w.getTile(player.x / size - 1, player.y / size) != 0)) || ((player.facing == FlxObject.RIGHT) && (w.getTile(1 + player.x / size, player.y / size) != 0))) {
						player.resize(size / 10);
						w.setTile(player.x / size + ((player.facing == FlxObject.RIGHT)?(1):( -1)), player.y / size, 0, true);
					}
				}
			}
		}
		
		// YEP, working
		private function mobCol(pl:Player, mob:BasicObj):void {
			var tmp:Number;
			if (((pl.y + pl.height - 2) <= mob.y) && !mob.flickering && (pl.velocity.y >= 0)) {
				tmp = pl.size / ((mob.size > 1)?(mob.size):(1)) / 10;
				mob.resize( -tmp);
			}
			//else if (pl.eat && !mob.flickering && (pl.size >= mob.size)) {
			//	if ((pl.x < mob.x)&& (pl.facing == FlxObject.RIGHT)) {
			else if ( (pl.eat && !mob.flickering && (pl.size >= mob.size)) && (((pl.x < mob.x) &&
			(pl.facing == FlxObject.RIGHT))||((pl.x > mob.x) && (pl.facing == FlxObject.LEFT)))) {
					pl.resize(mob.size / 10);
					mob.kill();
					FlxG.loadSound(sfx, 1.0, false, false, true);
			}
			//	else if ((pl.x > mob.x) && (pl.facing == FlxObject.LEFT)) {
			//		pl.resize(mob.size / 10);
			//		mob.kill();
			//	}
			//}
			else if (mob.bad && !mob.flickering) {
				tmp = mob.size / ((pl.size > 1)?(pl.size):(1)) / 10;
				pl.resize( -tmp);
				if (pl.size <= 0) {
					pl.kill();
				}
			}
		}
		
		public function setState(n:uint = 0):void {
			mob.callAll("kill");
			world.loadMap(FlxTilemap.arrayToCSV(uglyStageCreate(n), 32), tile01, 8, 8);
			world.setDirty();
		}
		
